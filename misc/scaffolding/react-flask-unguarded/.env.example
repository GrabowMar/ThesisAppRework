# Environment Configuration for Unguarded Generation Mode
# Copy to .env and customize

# Project identification
PROJECT_NAME=myapp

# Port configuration (managed by generation system)
BACKEND_PORT=5000
FRONTEND_PORT=8000

# Flask configuration
FLASK_ENV=production
# FLASK_DEBUG=0

# CORS (update for production)
CORS_ORIGINS=http://localhost:8000

# Database (SQLite by default, model may choose differently)
# DATABASE_URL=sqlite:////app/data/app.db

# Security (model should generate these)
# SECRET_KEY=change-this-in-production
# JWT_SECRET_KEY=change-this-jwt-secret
